  "devDependencies": {
    "acorn": "^6.4.1",
    "prettier": "^2.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "xo": "^0.28.0"
  },
  "xo": {
    "envs": [
      "amd"
    ],
    "globals": [
      "it",
      "describe"
